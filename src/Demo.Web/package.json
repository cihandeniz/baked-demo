{
  "name": "demo-web",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "setup": "nuxt prepare && npm run lint",
    "predev": "npm run setup --env=development",
    "dev": "nuxt dev --dotenv .env.development --host",
    "prebuild": "cross-env-shell npm run setup --env=$npm_config_env",
    "build": "cross-env-shell nuxi generate --log-level silent --dotenv .env.$npm_config_env",
    "build:Development": "npm run build --env=development",
    "prelocal": "npm run build:Development",
    "local": "npx http-server .output/public -p 3000",
    "postinstall": "nuxi prepare"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.12.1",
    "cross-env": "10.1.0",
    "sass": "1.97.1"
  },
  "dependencies": {
    "@mouseless/baked": "1.1.1",
    "@nuxtjs/i18n": "10.2.1",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@primeuix/themes": "2.0.2",
    "nuxt": "^4.2.2"
  }
}
