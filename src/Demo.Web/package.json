{
  "name": "demo-web",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "setup": "npm run lint",
    "prebuild": "cross-env-shell npm run setup --env=$npm_config_env",
    "build": "cross-env-shell nuxi generate --log-level silent --dotenv .env.$npm_config_env",
    "build:Development": "npm run build --env=development",
    "predev": "npm run setup --env=development",
    "dev": "nuxi dev --dotenv .env.development",
    "postinstall": "nuxi prepare"
  },
  "devDependencies": {
    "@nuxt/devtools": "2.6.2",
    "@nuxt/eslint": "1.7.1",
    "@nuxtjs/tailwindcss": "6.14.0",
    "cross-env": "7.0.3",
    "eslint": "9.32.0",
    "sass": "1.89.2"
  },
  "dependencies": {
    "@primeuix/themes": "1.2.1",
    "baked-recipe-admin": "0.17.0",
    "nuxt": "3.17.7",
    "vue": "3.5.17",
    "vue-router": "4.5.1"
  }
}
